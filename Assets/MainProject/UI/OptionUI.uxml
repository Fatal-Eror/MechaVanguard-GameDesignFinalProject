                    <ui:Slider picking-mode="Ignore" label="Mouse Y Sensitivity" value="42" high-value="100" name="YSlider" style="font-size: 25px; -unity-text-outline-width: 0; margin-top: 5px; margin-right: 5px; margin-bottom: 5px; margin-left: 5px;" />
                    <ui:Slider picking-mode="Ignore" label="Mouse X Sensitivity" value="42" high-value="100" name="XSlider" style="font-size: 25px; margin-top: 5px; margin-right: 5px; margin-bottom: 5px; margin-left: 5px;" />
                </ui:VisualElement>
                <ui:VisualElement name="ButtomBox" style="flex-grow: 1; margin-top: 20px; margin-right: 20px; margin-bottom: 20px; margin-left: 20px; height: 75%; justify-content: center; align-items: center; align-self: auto;">
                    <ui:Button text="Leave Room" parse-escape-sequences="true" display-tooltip-when-elided="true" name="Leave" style="width: 70%; border-top-left-radius: 5px; border-top-right-radius: 5px; border-bottom-right-radius: 5px; border-bottom-left-radius: 5px; background-color: rgb(48, 64, 67); -unity-text-outline-color: rgba(255, 255, 255, 0); color: rgb(255, 255, 255); border-top-width: 5px; border-right-width: 5px; border-bottom-width: 5px; border-left-width: 5px; border-left-color: rgb(250, 250, 250); border-right-color: rgb(250, 250, 250); border-top-color: rgb(250, 250, 250); border-bottom-color: rgb(250, 250, 250); font-size: 16px; letter-spacing: 30px; height: auto; align-self: auto; margin-top: 10px; margin-right: 10px; margin-bottom: 10px; margin-left: 10px; align-items: center; justify-content: center;" />
                    <ui:Button text="Back To Lobby" parse-escape-sequences="true" display-tooltip-when-elided="true" name="Back" style="width: 70%; border-top-left-radius: 5px; border-top-right-radius: 5px; border-bottom-right-radius: 5px; border-bottom-left-radius: 5px; background-color: rgb(48, 64, 67); -unity-text-outline-color: rgba(255, 255, 255, 0); color: rgb(255, 255, 255); border-top-width: 5px; border-right-width: 5px; border-bottom-width: 5px; border-left-width: 5px; border-left-color: rgb(250, 250, 250); border-right-color: rgb(250, 250, 250); border-top-color: rgb(250, 250, 250); border-bottom-color: rgb(250, 250, 250); font-size: 16px; letter-spacing: 30px; height: auto; align-self: auto; margin-top: 10px; margin-right: 10px; margin-bottom: 10px; margin-left: 10px;" />
                    <ui:Button text="Exit Game" parse-escape-sequences="true" display-tooltip-when-elided="true" name="Exit" style="width: 70%; border-top-left-radius: 5px; border-top-right-radius: 5px; border-bottom-right-radius: 5px; border-bottom-left-radius: 5px; background-color: rgb(48, 64, 67); -unity-text-outline-color: rgba(255, 255, 255, 0); color: rgb(255, 255, 255); border-top-width: 5px; border-right-width: 5px; border-bottom-width: 5px; border-left-width: 5px; border-left-color: rgb(250, 250, 250); border-right-color: rgb(250, 250, 250); border-top-color: rgb(250, 250, 250); border-bottom-color: rgb(250, 250, 250); font-size: 16px; letter-spacing: 30px; height: auto; align-self: auto; margin-top: 10px; margin-right: 10px; margin-bottom: 10px; margin-left: 10px;" />
